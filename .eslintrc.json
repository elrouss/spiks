{
  "env": {
    "browser": true,
    "es2019": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["build", "public", "node_modules", "tokens"],
  "rules": {
    "prefer-const": "error",
    "no-console": "warn"
  }
}
